{
  "title": "初学者介绍",
  "author" : "王滕辉",
  "description": "初学者是基于spring boot的一套整合开发方案",
  "language": "zh-hans",
  "plugins": [
    "disqus",
    "github",
    "-highlight", 
	"prism", 
	"prism-themes",
	"github@^2.0.0",
	"github-buttons@2.1.0",
	"-lunr",
	"-search",
    "search-pro",
	"book-summary-scroll-position-saver",
	"include-codeblock",
	"codeblock-filename",
	"donate",
	"favicon@^0.0.2",
	"-sharing",
	"sharing-plus@^0.0.2",
	"expandable-chapters-small",
	"sectionx",
	"bilibili",
	"multipart"

  ],
  "pluginsConfig": {
	"prism": {
		"css": [
			"prism-themes/themes/prism-duotone-dark.css"
		]
	},
	"theme-default": {
		"showLevel": true
	},
	"donate": {
        "wechat": "./document/wechat.png",
        "alipay": "./document/alipay.jpg",
        "title": "",
        "button": "打赏",
        "alipayText": "",
        "wechatText": ""
    },
	"favicon": {
        "shortcut":  "./document/favicon_16x16.ico",
        "bookmark": "./document/favicon_16x16.ico",
        "appleTouch": "./document/favicon_16x16.ico",
        "appleTouchMore": {
            "120x120":  "./document/favicon_16x16.ico",
            "180x180":  "./document/favicon_16x16.ico"
        }
    },
	"sharing": {
       "douban": false,
       "facebook": false,
       "google": true,
       "hatenaBookmark": false,
       "instapaper": false,
       "line": true,
       "linkedin": false,
       "messenger": false,
       "pocket": false,
       "qq": true,
       "qzone": true,
       "stumbleupon": false,
       "twitter": false,
       "viber": false,
       "vk": false,
       "weibo": true,
       "whatsapp": false,
       "all": [
           "facebook", "google", "twitter",
           "weibo", "instapaper", "linkedin",
           "pocket", "stumbleupon"
