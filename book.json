{
  "title": "初学者介绍",
  "author" : "王滕辉",
  "description": "初学者是基于spring boot的一套整合开发方案",
  "language": "zh-hans",
  "plugins": [
    "github", 
    "-highlight", 
	"prism", 
    "prism-themes","chart",
    "-lunr","mp3-player","code",
	"-search",
    "search-pro", "include-codeblock",
    "ace",
	"codeblock-filename",
	"donate",
	"custom-favicon",
	"-sharing",
	"sharing-plus",
	"expandable-chapters-small",
	"sectionx",
    "bilibili",
    "multipart"

  ],
  "pluginsConfig": {
	"prism": {
		"css": [
            "prism-themes/themes/prism-duotone-dark.css",
            "prism-themes/themes/prism-duotone-earth.css",
            "prism-themes/themes/prism-pojoaque.css"
        ], 
        "ignore": [
            "mermaid",
            "eval-js",
            "chart",
            "mp3"
        ]
	},
	"theme-default": {
		"showLevel": true
    },
    "github": {
        "url": "https://github.com/514840279/danyuan-application"
    },
	"donate": {
        "wechat": "/document/wechat.png",
        "alipay": "/document/alipay.jpg",
        "title": "",
        "button": "打赏",
        "alipayText": "",
        "wechatText": ""
    },
    "favicon": "/document/favicon_16x16.ico",
    "include-codeblock": {
        "template": "ace"
    },
    "sharing": {
       "douban": false,
       "facebook": false,
       "google": false,
       "hatenaBookmark": false,
       "instapaper": false,
       "line": false,
       "linkedin": false,
       "messenger": false,
       "pocket": false,
       "qq": true,
       "qzone": true,
       "stumbleupon": false,
       "twitter": false,
       "viber": false,
       "vk": false,
       "weibo": true,
       "whatsapp": false,
       "all": [
           "facebook", "google", "twitter",
           "weibo", "instapaper", "linkedin",
           "pocket", "stumbleupon"
       ]
    },
	"sectionx": {
		"tag": "b"
	}
	
  }
}